msgstr "[[storefront]]：[[usergroup]]：开始价格表生成"

msgctxt "Languages::customer_price_list.ending_generate_price_list"
msgid "[[storefront]]: [[usergroup]]: ending price list generation"
msgstr "[[storefront]]：[[usergroup]]：结束价目表生成"

