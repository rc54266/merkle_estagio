msgid "[[storefront]]: [[usergroup]]: starting price list generation"
msgstr "[[storefront]]: [[usergroup]]: 가격 목록 생성 시작"

msgctxt "Languages::customer_price_list.ending_generate_price_list"
msgid "[[storefront]]: [[usergroup]]: ending price list generation"
msgstr "[[storefront]]: [[usergroup]]: 가격 목록 생성 종료"

msgctxt "Languages::customer_price_list.generate_price_list_error"
msgid "[[storefront]]: [[usergroup]]: [error]"
msgstr "[[storefront]]: [[usergroup]]: [error]"

